<Window x:Class="NaukaSlowek.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NaukaSlowek"
        mc:Ignorable="d"
        Title="Ustawienia" Height="450" Width="300" Loaded="Window_Loaded">
    <Grid>
        <DockPanel>
            <StackPanel Margin="10 10 0 10" DockPanel.Dock="Left" Width="200">
                <DataGrid x:Name="GridValues" CanUserReorderColumns="False" CanUserResizeColumns="False" CanUserResizeRows="False"></DataGrid>
            </StackPanel>
            <StackPanel Margin="10 10 10 10" DockPanel.Dock="Right">
                <Button x:Name="ButtonApply" Click="ButtonApply_Click">Zatwierdź</Button>
                <Button x:Name="ButtonCancel" Margin="0 15 0 0">Anuluj</Button>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
