<Window x:Class="NaukaSlowek.ExecuteTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NaukaSlowek"
        mc:Ignorable="d"
        Title="Nauka Słówek - Test" Height="600" Width="450" Closed="Window_Closed">
    <Grid>
        <DockPanel LastChildFill="True">
            <StackPanel x:Name="StackCategory" DockPanel.Dock="Top" Visibility="Hidden">
               <Label x:Name="LabelCategory" Content="Kategoria zwierzęta"/> 
            </StackPanel>
            
            <Button x:Name="ButtonFinishTest" Content="Zakończ" DockPanel.Dock="Bottom"/>
            <DataGrid x:Name="WordTestGrid" Height="Auto" />
        </DockPanel>
        
    </Grid>
</Window>
